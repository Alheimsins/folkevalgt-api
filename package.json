{
  "name": "folkevalgt-api",
  "description": "Proof of concept av felles api for kontaktinformasjon til folkevalgte",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=10.13.0"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "start": "micro",
    "dev": "micro-dev",
    "deploy": "npm test && now && now alias",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "micro",
    "folkevalgt",
    "api",
    "åpne data"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alheimsins/folkevalgt-api.git"
  },
  "bugs": {
    "url": "https://github.com/Alheimsins/folkevalgt-api/issues"
  },
  "homepage": "https://github.com/Alheimsins/folkevalgt-api#readme",
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.2",
    "husky": "1.1.3",
    "micro-dev": "3.0.0",
    "nyc": "13.1.0",
    "standard": "12.0.1",
    "test-listen": "1.1.0"
  },
  "dependencies": {
    "@alheimsins/acos-innsyn": "1.0.0",
    "axios": "0.18.0",
    "cors": "2.8.5",
    "finalhandler": "1.1.1",
    "markdown-it": "8.4.2",
    "micro": "9.3.3",
    "opengov-meetings": "5.2.0",
    "router": "1.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  }
}
