  "license": "MIT",
  "private": true,
  "author": {
    "name": "Geir Gåsodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "contributors": [
    {
      "name": "Hallvard Nygård",
      "url": "http://hnygard.no/"
    }
  ],
  "main": "index.js",
  "engines": {
    "node": ">=12.18.3"
  },
  "scripts": {
    "test": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "update-kommuner": "node utils/update-kommuner.js",
    "start": "micro",
    "dev": "vc dev",
    "deploy": "npm test && vc --prod",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install"
  },
  "keywords": [
    "microservice",
    "folkevalgt",
    "api",
    "åpne data"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alheimsins/folkevalgt-api.git"
  },
  "bugs": {
    "url": "https://github.com/Alheimsins/folkevalgt-api/issues"
  },
  "homepage": "https://github.com/Alheimsins/folkevalgt-api#readme",
  "devDependencies": {
    "ava": "3.13.0",
    "coveralls": "3.1.0",
    "nyc": "15.1.0",
    "standard": "15.0.1"
  },
  "dependencies": {
    "@alheimsins/acos-innsyn": "2.0.4",
    "axios": "0.20.0",
    "opengov-meetings": "6.0.2"
  }
}
