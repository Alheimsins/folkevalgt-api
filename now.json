{
  "version": 2,
  "name": "folkevalgt-api",
  "alias": [
    "folkevalgt.alheimsins.net"
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "builds": [
    { 
      "src": "/index.js",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}